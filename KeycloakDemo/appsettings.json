{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Authority": "http://192.168.1.30:8080/realms/myrealm",
    "MetadataAddress": "http://192.168.1.30:8080/realms/myrealm/.well-known/openid-configuration",
    "ClientId": "public",
    "ClientSecret": "",
    "ResponseType": "code",
    "SaveTokens": true,
    "GetClaimsFromUserInfoEndpoint": true,
    "Scope": [ "openid", "profile", "email" ]
  }
}
